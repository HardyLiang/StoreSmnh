
page {
  background: #f5f5f5;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  font-size: 16px;
  min-height: 100%;
  width: 100%;
  height: 100%;
  overflow-x: hidden;
   -webkit-user-select: none;
    user-select: none;
}
.text-oneLine{
   overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.text-twoLine{
   display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
checkbox{width: 24px;}
checkbox .wx-checkbox-input {
  border-radius: 50%; /* 圆角 */
  width: 46rpx;
  height: 46rpx;
}

/* 选中后的 背景样式 */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: none;
  background: #48c23d;
}
/* 选中后的 对勾样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 46rpx; 
  height: 46rpx; 
  line-height: 46rpx;
  text-align: center;
  font-size: 30rpx; 
  color: #fff; 
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
  background-color: #48c23d;
  border: 1rpx solid #48c23d;
}
/* 向右箭头 */
.navigator-arrow {
  padding-right: 26rpx;
  position: relative;
}

.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #999;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}